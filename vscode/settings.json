{
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": true,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.detectIndentation": false,
  "editor.find.globalFindClipboard": true,
  "editor.fontFamily": "SFMono-Regular, Menlo, Monaco, 'Courier New', monospace",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.renderWhitespace": "selection",
  "editor.scrollBeyondLastLine": false,
  "editor.selectionHighlight": false,
  "editor.semanticHighlighting.enabled": true,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "entity.name.type",
          "keyword",
          "support",
          "variable.parameter"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "emmet.variables": {"charset": "utf-8"},
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.colors": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "yaml": false
  },
  "javascript.validate.enable": false,
  "json.schemas": [],
  "rust-analyzer.check.command": "clippy",
  "scm.defaultViewMode": "tree",
  "scm.providerCountBadge": "auto",
  "scm.repositories.sortOrder": "name",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.fontFamily": "MesloLGMNFM-Regular",
  "terminal.integrated.minimumContrastRatio": 1,
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.editor.decorations.colors": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "workbench.startupEditor": "newUntitledFile",
  "[css]": {
    "editor.formatOnSave": true
  },
  "[dockercompose]": {
    "editor.quickSuggestions": {
      "strings": "on"
    }
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.formatOnSave": true
  },
  "[rust]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 4
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.ignoreRegExpList": [
    "\\$[a-zA-Z_]+",
    "\\['[a-zA-Z-_]+'\\]",
    "=\"[a-zA-Z-_, ]+\""
  ],
  "cSpell.language": "en-GB",
  "cSpell.userWords": [
    "admin",
    "analytics",
    "apps",
    "artifact",
    "artifacts",
    "assistive",
    "Atlassian",
    "bonafide",
    "buymeasoda",
    "codemod",
    "codemods",
    "componentry",
    "composability",
    "contrarian",
    "debuggability",
    "deconstruct",
    "deconstructed",
    "deprioritising",
    "destructure",
    "dialog",
    "dotfiles",
    "editability",
    "engineerable",
    "gnarly",
    "impactful",
    "introspectability",
    "jira",
    "leveraging",
    "linting",
    "mentoring",
    "mentorship",
    "mkdir",
    "monitorable",
    "orchestrators",
    "overspecify",
    "parameterised",
    "parsable",
    "pathing",
    "performant",
    "pinboard",
    "platformisation",
    "powerline",
    "prefs",
    "proactively",
    "queryable",
    "referenceable",
    "refetch",
    "relatable",
    "repurposed",
    "repurposing",
    "requestable",
    "sandboxed",
    "scaffolded",
    "scriptable",
    "setup",
    "startup",
    "subsort",
    "subsorted",
    "subsorting",
    "symlink",
    "theming",
    "trifecta",
    "uncomment",
    "unmount",
    "walkthrough",
    "wayfinding",
    "wireframe",
    "workflow",
    "workflows",
    "worklet",
    "worklets"
  ]
}
