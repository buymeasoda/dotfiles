{
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": true,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.detectIndentation": false,
  "editor.find.globalFindClipboard": true,
  "editor.minimap.showSlider": "always",
  "editor.renderWhitespace": "selection",
  "editor.scrollBeyondLastLine": false,
  "editor.selectionHighlight": false,
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "explorer.confirmDragAndDrop": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.fontFamily": "MesloLGM-RegularForPowerline",
  "workbench.colorCustomizations": {
    "editorBracketMatch.border": "#cbcbcb",
    "terminal.ansiBlack": "#323334",
    "terminal.ansiRed": "#da6a6a",
    "terminal.ansiGreen": "#63a14c",
    "terminal.ansiYellow": "#cfb30d",
    "terminal.ansiBlue": "#5999b9",
    "terminal.ansiMagenta": "#b578cc",
    "terminal.ansiCyan": "#258595",
    "terminal.ansiWhite": "#cdcdcd",
    "terminal.ansiBrightBlack": "#8c8c8c",
    "terminal.ansiBrightRed": "#df968e",
    "terminal.ansiBrightGreen": "#cdef45",
    "terminal.ansiBrightYellow": "#fee45e",
    "terminal.ansiBrightBlue": "#9cd8f3",
    "terminal.ansiBrightMagenta": "#faafff",
    "terminal.ansiBrightCyan": "#79ded8",
    "terminal.ansiBrightWhite": "#f7f7f7",
    "terminal.background": "#17181a",
    "terminal.foreground": "#979b9f",
    "terminal.selectionBackground": "#9cd8f355",
    "terminalCursor.background": "#ffffff",
    "terminalCursor.foreground": "#a6a6a6"
  },
  "workbench.colorTheme": "Bluloco Light",
  "workbench.iconTheme": "file-icons-colourless",
  "workbench.startupEditor": "newUntitledFile",
  "prettier.bracketSpacing": false,
  "prettier.jsxBracketSameLine": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "[css]": {
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.formatOnSave": true
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["comment"],
        "settings": {"fontStyle": "italic"}
      }
    ]
  },
  "emmet.variables": {
    "charset": "utf-8"
  },
  "cSpell.ignoreRegExpList": [
    "\\$[a-zA-Z_]+",
    "\\['[a-zA-Z-_]+'\\]",
    "=\"[a-zA-Z-_, ]+\""
  ],
  "cSpell.language": "en-GB",
  "cSpell.userWords": [
    "assistive",
    "buymeasoda",
    "codemod",
    "codemods",
    "dotfiles",
    "mkdir",
    "performant",
    "pinboard",
    "powerline",
    "prefs",
    "refetch",
    "symlink",
    "uncomment",
    "unmount"
  ]
}
