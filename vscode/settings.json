{
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": true,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.detectIndentation": false,
  "editor.find.globalFindClipboard": true,
  "editor.fontFamily": "SFMono-Regular, Menlo, Monaco, 'Courier New', monospace",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.renderWhitespace": "selection",
  "editor.scrollBeyondLastLine": false,
  "editor.selectionHighlight": false,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "entity.name.type",
          "keyword",
          "support",
          "variable.parameter"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "emmet.variables": {"charset": "utf-8"},
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.colors": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "github.copilot.advanced": {},
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "yaml": false
  },
  "javascript.validate.enable": false,
  "jest.autoRun": "off",
  "json.schemas": [],
  "rust-analyzer.check.command": "clippy",
  "search.exclude": {
    "**/__generated-mocks": true,
    "**/build": true,
    "**/bundles-report": true
  },
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.fontFamily": "MesloLGM-RegularForPowerline",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "terminal.ansiBlack": "#323334",
    "terminal.ansiRed": "#da6a6a",
    "terminal.ansiGreen": "#63a14c",
    "terminal.ansiYellow": "#cfb30d",
    "terminal.ansiBlue": "#5999b9",
    "terminal.ansiMagenta": "#b578cc",
    "terminal.ansiCyan": "#258595",
    "terminal.ansiWhite": "#cdcdcd",
    "terminal.ansiBrightBlack": "#8c8c8c",
    "terminal.ansiBrightRed": "#df968e",
    "terminal.ansiBrightGreen": "#cdef45",
    "terminal.ansiBrightYellow": "#fee45e",
    "terminal.ansiBrightBlue": "#9cd8f3",
    "terminal.ansiBrightMagenta": "#faafff",
    "terminal.ansiBrightCyan": "#79ded8",
    "terminal.ansiBrightWhite": "#f7f7f7",
    "terminal.background": "#17181a",
    "terminal.foreground": "#979b9f",
    "terminal.selectionBackground": "#9cd8f355",
    "terminalCursor.background": "#ffffff",
    "terminalCursor.foreground": "#a6a6a6"
  },
  "workbench.colorTheme": "Dracula Theme",
  "workbench.editor.decorations.colors": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.iconTheme": "file-icons",
  "workbench.startupEditor": "newUntitledFile",
  "[css]": {
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.formatOnSave": true
  },
  "[rust]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 4
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.ignoreRegExpList": [
    "\\$[a-zA-Z_]+",
    "\\['[a-zA-Z-_]+'\\]",
    "=\"[a-zA-Z-_, ]+\""
  ],
  "cSpell.language": "en-GB",
  "cSpell.userWords": [
    "admin",
    "analytics",
    "apps",
    "artifact",
    "artifacts",
    "assistive",
    "Atlassian",
    "bonafide",
    "buymeasoda",
    "codemod",
    "codemods",
    "componentry",
    "composability",
    "contrarian",
    "debuggability",
    "deconstruct",
    "deconstructed",
    "deprioritising",
    "destructure",
    "dialog",
    "dotfiles",
    "editability",
    "engineerable",
    "gnarly",
    "impactful",
    "introspectability",
    "jira",
    "leveraging",
    "linting",
    "mentoring",
    "mentorship",
    "mkdir",
    "monitorable",
    "orchestrators",
    "overspecify",
    "parameterised",
    "performant",
    "pinboard",
    "platformisation",
    "powerline",
    "prefs",
    "proactively",
    "queryable",
    "referenceable",
    "refetch",
    "relatable",
    "repurposed",
    "repurposing",
    "requestable",
    "sandboxed",
    "scaffolded",
    "scriptable",
    "setup",
    "startup",
    "subsort",
    "subsorted",
    "subsorting",
    "symlink",
    "theming",
    "trifecta",
    "uncomment",
    "unmount",
    "walkthrough",
    "wayfinding",
    "wireframe",
    "workflow",
    "workflows",
    "worklet",
    "worklets"
  ]
}
